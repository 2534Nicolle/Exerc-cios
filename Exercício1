package lpa2;
import java.util.ArrayList;
public class Number {

	public static void main(String[] args) {
		
        ArrayList<Integer> numeros = new ArrayList<>();
		
		for (int i = 0; i < 10; i++) {
			numeros.add(i);
		}
		
		System.out.println("ANTES");
		for (int numero : numeros) {
			System.out.println(numero);
		}
		numeros.remove(4);
		
		System.out.println("**************");
		System.out.println("DEPOIS");
		
		for (int numero : numeros) {
			System.out.println(numero);
	}
	}
}
